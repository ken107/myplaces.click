{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "member-access": false,
    "no-string-literal": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "align": false,
    "no-namespace": false
  },
  "rulesDirectory": []
}